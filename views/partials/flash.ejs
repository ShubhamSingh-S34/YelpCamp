<% if(success.length){ %>
    <div class="alert alert-success alert-dismissible fade show" role="allert">
        <%= success %>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
    <% } %>



        <% if(error.length){ %>
            <div class="alert alert-danger alert-dismissible fade show" role="allert">
                <%= error %>
                    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
            </div>
            <% } %>